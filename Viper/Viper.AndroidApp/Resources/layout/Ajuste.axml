        android:layout_height="wrap_content" />
    <TextView
        android:text="Cantidad Actual"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/textView1"
        android:layout_marginTop="52.0dp" />
</LinearLayout>